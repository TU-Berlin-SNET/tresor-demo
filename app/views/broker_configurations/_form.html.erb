<%= form_for(@broker_configuration) do |f| %>
    <%= render 'shared/error_messages', :object => f.object %>

    <table>
      <tr>
        <th></th>
        <th></th>
      </tr>
      <tbody>
      <tr>
        <td>
          <%= f.label :name %>
        </td>
        <td>
          <%= f.text_field :name %>
        </td>
      </tr>
    </table>
    <div class="actions">
      <%= f.submit t(:save) %>
    </div>
<% end %>
